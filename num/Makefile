OCAMLLIBS?=-I .\
  -I ../elliptic\
  -I ../List\
  -I ../PrimalityTest\
  -I ../Z\
  -I ../N\
  -I ../Tactic
COQSRCLIBS?=-I $(COQLIB)kernel -I $(COQLIB)lib \
  -I $(COQLIB)library -I $(COQLIB)parsing \
  -I $(COQLIB)pretyping -I $(COQLIB)interp \
  -I $(COQLIB)proofs -I $(COQLIB)tactics \
